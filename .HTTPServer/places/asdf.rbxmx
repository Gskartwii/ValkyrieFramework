					<string name="Name">ClientScript</string>
					<ProtectedString name="Source"><![CDATA[local sidebar			= script.Parent.PlayersFrame;
local infoFrame			= script.Parent.InfoFrame;
local rstep				= game:GetService("RunService").RenderStepped;
local header			= infoFrame.Username;
local friendsheader		= infoFrame.Friends;
local achvheader		= infoFrame.Achievements;
local userImage			= infoFrame.UserImage;
local fieldsContainer	= infoFrame.FieldsContainer;
local friendsContainer	= infoFrame.FriendsContainer;
local achvContainer		= infoFrame.AchievementsContainer;
local sanimation		= script.Parent.SideAnimation;
local easingFunctions	= {
inQuad = function(t, b, c, d)
		t = t / d
		return c * math.pow(t, 2) + b
	end
}

local function tweenFromLeft(obj, origpos)
	rstep:wait();
	local elapsed		= 0;
	local beginVal		= obj.Position.X.Offset;
	local endVal		= origpos;
	local duration		= 2 / 3;
	local changeVal		= endVal - beginVal;
	
	for i = 1, 40 do
		elapsed			= elapsed + 1 / 60;
		local step		= easingFunctions.inQuad(elapsed, beginVal, changeVal, duration);
		obj.Position 	= UDim2.new(obj.Position.X.Scale, step, obj.Position.Y.Scale, obj.Position.Y.Offset);
		
		rstep:wait();
	end
end

local function animateOpen(neworigpos)
	local sidebarnames	= sidebar:GetChildren();
	local origpos		= {};
	for _, name in next, sidebarnames do
		origpos[name]	= name.AbsolutePosition.X;
		name.Position	= UDim2.new(0, -1 * name.AbsoluteSize.X, name.Position.Y.Scale, name.Position.Y.Offset);
	end
	local objs 					= {header, userImage, achvheader, friendsheader, fieldsContainer, achvContainer, friendsContainer};

	for _, obj in next, objs do
		origpos[obj]			= obj.Position.X.Offset;
		obj.Position			= UDim2.new(obj.Position.X.Scale, infoFrame.AbsoluteSize.X, obj.Position.Y.Scale, obj.Position.Y.Offset);
	end

	for _, name in next, sidebarnames do
		rstep:wait();
		spawn(function()
			local elapsed		= 0;
			local beginVal		= name.Position.X.Offset;
			local endVal		= origpos[name];
			local duration		= 1 / 2;
			local changeVal		= endVal - beginVal;
			
			for i = 1, 30 do
				elapsed			= elapsed + 1 / 60;
				local step		= easingFunctions.inQuad(elapsed, beginVal, changeVal, duration);
				name.Position 	= UDim2.new(name.Position.X.Scale, step, name.Position.Y.Scale, name.Position.Y.Offset);
				
				rstep:wait();
			end
		end);
		
		if name:FindFirstChild("SideAnimation") then -- SelectedPlayer does NOT have it.
			name.SideAnimation.Disabled	= false;
		end
	end
	
	sanimation.Disabled			= false; -- Connect all events
	
	origpos						= neworigpos or origpos;
	
	for _, obj in next, objs do
		rstep:wait();
		spawn(function()
			tweenFromLeft(obj, origpos[obj]);
		end);
	end
end

animateOpen();]]></ProtectedString>
