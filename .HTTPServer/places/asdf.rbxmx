	seemoreinst.Position	= UDim2.new(seemoreinst.Position.X.Scale, friends * 100, seemoreinst.Position.Y.Scale, seemoreinst.Position.Y.Offset);
	seemoreinst.Parent	= friendsContainer;
	
	-- Handle clicking here
end

function module.addSeeMoreAchievement()
