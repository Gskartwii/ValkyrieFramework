function module.pushSidebar(id, name, status, tbl, index)
	local item						= side[status]:Clone();
	item.Text						= name;
	item.Position					= UDim2.new(item.Position.X.Scale, item.Position.X.Offset, item.Position.Y.Scale, sidebaritems * 30);
	local side_bg_extend			= Instance.new("Frame", item.BackgroundExtend);
	side_bg_extend.Size				= UDim2.new(0,0,1,0);
	side_bg_extend.BackgroundColor3 = item.BackgroundColor3;
	side_bg_extend.BorderSizePixel	= 0;
	item.Parent						= playerFrameTemplate;
	if item							~= playerFrameTemplate:FindFirstChild"SelectedPlayer" then
		local clickHandler			= Instance.new("RemoteFunction", item);
		clickHandler.Name			= "ClickHandler";
		clickHandler.OnServerInvoke	= function(player)
			--[[wait(3);
			-- Fetch new data from the server, pls
			module.build(player.Name, tbl, { -- Placeholder for now
			  [1] = 1427650061,
			  [2] = { 36537369, "gskw", false },
			  [3] = { { "16min", "1d", "2h 51min" }, { "999", "1427639731", "1427639752" } },
			  [4] = { { "basic_bronze", "test", "Basic Bronze Achievement", "This achievement is awarded to you once you complete the first quest.\n\n    Should not be challenging at all!", 1, 231496690 }, { "basic_bronze_master", "test", "Epic Bronze Achievement", "The super duper achievement!!", 2, 186253727 } },
			  [5] = 3,
			  [6] = { { 31442735, "destrojet", false }, { 3838167, "Naruto6192", false }, { 65765854, "eLunate", false }, { 58718869, "HttpGetAsync", false }, { 61441223, "Temppeliherra", false }, { 19641443, "michaelkeysgate", false }, { 28893687, "storadow", false }, { 37586818, "averywalters11", false }, { 27578143, "Ninja560000", true, "test" } }
			}, sidebaritems);]]
			
			local body				= ("id=%d"):format(id);
		end
	end
	
	sidebaritems						= sidebaritems + 1;
end

function module.pushAchievement(id, icon, name, reward)
	local achvinst		= achievement:Clone();
	achvinst.Image		= "rbxassetid://" .. icon;
	achvinst.Position	= UDim2.new(achvinst.Position.X.Scale, achievements * 100, achvinst.Position.Y.Scale, achvinst.Position.Y.Offset);
	achvinst.Parent		= achvContainer;
	local hoveredReward	= rewardOverlay:Clone();
	hoveredReward.BackgroundTransparency 	= 1;
	hoveredReward.TextTransparency			= 1;
	hoveredReward.Text						= reward;
	hoveredReward.Parent					= achvinst;
	
	-- handle hovering here
	
	achievements			= achievements + 1;
end

function module.pushFriend(id, name, status)
	local friendinst	= friend:Clone();
	friendinst.Image		= "http://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&Format=Png&username=" .. name;
	friendinst.Position	= UDim2.new(friendinst.Position.X.Scale, friends * 100, friendinst.Position.Y.Scale, friendinst.Position.Y.Offset);
	friendinst.Parent	= friendsContainer;
	local username		= (status and friend_online or friend_offline):Clone();
	username.Text		= name;
	username.Parent		= friendinst; 
	
	-- handle clicking here	
	
	friends				= friends + 1;
end

function module.addSeeMoreFriend()
