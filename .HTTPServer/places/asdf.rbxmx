	seemoreinst.Position	= UDim2.new(seemoreinst.Position.X.Scale, friends * 100, seemoreinst.Position.Y.Scale, seemoreinst.Position.Y.Offset);
	seemoreinst.Parent	= achvContainer;
	
	-- Handle clicking here
end

function module.build(player, tbl, detailInfo, index)
	module.reClone();
	playerFrameTemplate:ClearAllChildren();
	achvContainer:ClearAllChildren();
	friendsContainer:ClearAllChildren();
	
	local function sortFunc(a, b)
		return a[2]:lower():byte(1, 1) < b[2]:lower():byte(1, 1);
	end
	
	local oldtbl		= {unpack(tbl)}; -- Copy the table
	tbl					= {};
	local temptbl		= {online = {}, offline = {}};
	for num, name in next, oldtbl do
		if index		== num then
			print("ins1", name[2]);
			table.insert(tbl, name);
		elseif name[3] 	== 1 then
			table.insert(temptbl.offline, name);
		else
			table.insert(temptbl.online, name);
		end
	end
	
	table.sort(temptbl.online, sortFunc);
	table.sort(temptbl.offline, sortFunc);
	
	for _, name in next, temptbl.online do
		print("ins2", name[2]);
		table.insert(tbl, name);
	end
	for _, name in next, temptbl.offline do
		print("ins3", name[2]);
		table.insert(tbl, name);
	end
	
	for num, listItem in next, tbl do
		module.pushSidebar(listItem[1], listItem[2], num == 1 and 3 or listItem[3] and 2 or 1, tbl, index);
	end
	
	local currTime		= detailInfo[1];
	local stats			= detailInfo[3];
	local selfInfo		= detailInfo[2];
	local achvInfo		= detailInfo[4];
	local friendInfo	= detailInfo[6];
	
	local name			= selfInfo[2];
	local isOnline		= selfInfo[3];
	local whereOnline	= selfInfo[5];
	local gidOnline		= selfInfo[4];
	local totalReward	= detailInfo[5];
	
	local formStats		= stats[1];
	local rawStats		= stats[2];
	
	local timeIngame	= formStats[1];
	local joined		= formStats[2];
	local lastOnline	= formStats[3];
	
	if isOnline then
		local color3	= Color3.new(102/255, 187/255, 106/255);
		onlineStatus.BackgroundColor3 = color3;
		onlineStatusName.BackgroundExtend.BackgroundColor3 = color3;
		onlineStatusName.BackgroundColor3 = color3;
	else
		local color3	= Color3.new(239/255, 83/255, 80/255);
		onlineStatus.BackgroundColor3 = color3;
		onlineStatusName.BackgroundExtend.BackgroundColor3 = color3;
		onlineStatusName.BackgroundColor3 = color3;
	end
	
	
	
	local oldFriendInfo		= friendInfo;
	local tempFriendInfo	= {online = {}, offline = {}};
	
	for _, friend in next, oldFriendInfo do
		if friend[3] then
			table.insert(tempFriendInfo.online, friend);
		else
			table.insert(tempFriendInfo.offline, friend);
		end
	end
	
	table.sort(tempFriendInfo.online, sortFunc);
	table.sort(tempFriendInfo.offline, sortFunc);
	
	friendInfo				= tempFriendInfo.online;
	for _, friend in next, tempFriendInfo.offline do
		table.insert(friendInfo, friend);
	end
	
	for i = 1, 10 do
		if not friendInfo[i] then
			break;
		end
		
		if i == 10 then
			module.addSeeMoreFriend();
		end
		
		module.pushFriend(unpack(friendInfo[i]));
	end
	
	for i = 1, 10 do
		if not achvInfo[i] then
			break;
		end
		
		if i == 10 then
			module.addSeeMoreAchv();
		end
		
		module.pushAchievement(achvInfo[i][1],
			 achvInfo[i][6],
			 achvInfo[i][3],
			 achvInfo[i][5]);
	end
	userImage.Image	= "http://www.roblox.com/Thumbs/Avatar.ashx?x=250&y=250&Format=Png&username=" .. name;
	
	localizer.localize(nil, "fi_fi", {
		["InfoFrame/FieldsContainer/OnlineStatusCont"] 	= isOnline and 1 or 2
	}, {
		["InfoFrame/FieldsContainer/OnlineStatusCont"] 	= {
			const		= true;
			args		= {whereOnline};
		};
		["InfoFrame/FieldsContainer/FieldCont2"]		= {
			const		= true;
			args		= {totalReward};
		};
		["InfoFrame/Username"]							= {
			const		= true;
			args		= {name};
		};
		["InfoFrame/FieldsContainer/FieldCont1"]		= {
			const		= true;
			args		= {timeIngame};
		};
		["InfoFrame/FieldsContainer/FieldCont3"]		= {
			const		= true;
			args		= {joined};
		};
		["InfoFrame/FieldsContainer/FieldCont4"]		= {
			const		= true;
			args		= {lastOnline};
		};
	}, {InfoFrame = infoFrameTemplate});
	
	local playerGui				= game.Players[player].PlayerGui;
	if playerGui:FindFirstChild("__Valkyrie_PlayerDisplay") then
		playerGui.__Valkyrie_PlayerDisplay:Destroy();
	end
	
	local scrGui				= Instance.new("ScreenGui", playerGui);
	local sanimation			= sideAnimation:Clone();
	sanimation.Parent			= scrGui; -- Do not enabled sanimation just yet...
	
	scrGui.Name					= "__Valkyrie_PlayerDisplay"; 
	playerFrameTemplate.Parent 	= scrGui;
	infoFrameTemplate.Parent	= scrGui;
	clientScript.Parent			= scrGui;
	localizer.Parent			= scrGui;
	clientScript.Disabled		= false;
end

return module;]]></ProtectedString>
