			<Item class="LocalScript" referent="RBX7616114609854676A320371191B2D3A9">
				<Properties>
					<bool name="Disabled">true</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">SideAnimation</string>
					<ProtectedString name="Source"><![CDATA[local sidebar			= script.Parent.PlayersFrame;
local infoFrame			= script.Parent.InfoFrame;
local achvContainer		= infoFrame.AchievementsContainer;
local fieldsContainer	= infoFrame.FieldsContainer;
local friendsContainer	= infoFrame.FriendsContainer;
local userImage			= infoFrame.UserImage;
local header			= infoFrame.Username;
local achvheader		= infoFrame.Achievements;
local friendsheader		= infoFrame.Friends;

local connections		= {};

local easingFunctions	= {
inQuint = function(t, b, c, d)
	t = t / d
	return c * math.pow(t, 5) + b
end;
inQuad = function(t, b, c, d)
		t = t / d
		return c * math.pow(t, 2) + b
	end};
local rstep				= game:GetService("RunService").RenderStepped;

local function tweenToLeft(obj)
	rstep:wait();
	local elapsed		= 0;
	local beginVal		= obj.Position.X.Offset;
	local endVal		= infoFrame.AbsoluteSize.X;
	local duration		= 1 / 2;
	local changeVal		= endVal - beginVal;
	
	for i = 1, 30 do
		elapsed			= elapsed + 1 / 60;
		local step		= easingFunctions.inQuad(elapsed, beginVal, changeVal, duration);
		obj.Position 	= UDim2.new(obj.Position.X.Scale, step, obj.Position.Y.Scale, obj.Position.Y.Offset);
		
		rstep:wait();
	end
end

local function animateClose()
	local sidebarnames	= sidebar:GetChildren();
	for _, name in next, sidebarnames do
		rstep:wait();
		spawn(function()
			local elapsed		= 0;
			local beginVal		= name.AbsolutePosition.X;
			local endVal		= 0 - name.AbsoluteSize.X;
			local duration		= 10 / 60;
			local changeVal		= endVal - beginVal;
			
			for i = 1, 10 do
				elapsed			= elapsed + 1 / 60;
				local step		= easingFunctions.inQuad(elapsed, beginVal, changeVal, duration);
				name.Position 	= UDim2.new(name.Position.X.Scale, step, name.Position.Y.Scale, name.Position.Y.Offset);
				
				rstep:wait();
			end
		end);
	end
	
	local objs 					= {header, userImage, achvheader, friendsheader, fieldsContainer, achvContainer, friendsContainer};

	local origpos				= {};
	for _, obj in next, objs do
		origpos[obj]			= obj.Position.X.Offset;
		rstep:wait();
		spawn(function()
			tweenToLeft(obj);
		end);
	end
	
	return origpos;
end

for _, item in next, sidebar:GetChildren() do
	if item.Name ~= "SelectedPlayer" then
		local side_bg_extend	= item.BackgroundExtend.Frame;
		table.insert(connections, item.InputEnded:connect(function(inpObject)
			if inpObject.UserInputType ~= Enum.UserInputType.MouseButton1 then return; end
			for _, connection in next, connections do
				connection:disconnect();
			end
			
			local elapsed		= 0;
			local beginVal		= item.Position.X.Offset;
			local endVal		= item.Position.X.Offset + 50;
			local duration		= 1 / 6;
			local changeVal		= endVal - beginVal;
			
			local color3tween	= {
				[item] 						= {beginVal = item.BackgroundColor3, endVal = Color3.new(66/255, 165/255, 245/255)}, 
				[side_bg_extend] 			= {beginVal = side_bg_extend.BackgroundColor3, endVal = Color3.new(66/255, 165/255, 245/255)}, 
				[item.BackgroundExtend]		= {beginVal = item.BackgroundExtend.BackgroundColor3, endVal = Color3.new(66/255, 165/255, 245/255)};
				[sidebar.SelectedPlayer]	= {beginVal = item.Parent.SelectedPlayer.BackgroundColor3, endVal = Color3.new(189/255, 189/255, 189/255)};
